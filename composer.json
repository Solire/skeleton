{
  "name": "solire/skeleton",
  "type": "project",
  "description": "",
  "keywords": [""],
  "license": "CC by-nc",
  "repositories": [
    {
      "type": "composer",
      "url": "http://satis.solire.fr/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "solire/install": "dev-feature/refonteback",
    "solire/lib": "dev-feature/refonteback",
    "solire/back": "dev-feature/refonteback",
    "solire/front": "dev-develop"
  },
  "require-dev": {
    "m6web/coke": "~2.0",
    "siwayll/phpcs-norme-dpsi": "~3.2"
  },
  "scripts": {
    "post-install-cmd": [
      "Solire\\Install\\Ask::init",
      "Solire\\Install\\Project::name",
      "Solire\\Install\\Ask::parameters",
      "Solire\\Install\\Db::create",
      "Solire\\Install\\Ask::write",
      "Solire\\Install\\Dir::allowPermissions",
      "Solire\\Install\\FrontEnd::install",
      "Solire\\Install\\Symlinks::create"
    ],
    "post-update-cmd": [
      "Solire\\Install\\Dir::allowPermissions",
      "Solire\\Install\\FrontEnd::install",
      "Solire\\Install\\Symlinks::create"
    ]
  },
  "extra": {
    "solire": {
      "allowPermissions": [
        "cache/twig",
        "upload",
        "upload/temp"
      ],
      "ask": "config/questions.yml",
      "parameters": {
        "config/local.yml": {},
        "config/main.yml": {}
      },
      "frontEnd": {
        "dirs": "config/symlinks.yml"
      },
      "db": [
        {
          "config": "config/local.yml",
          "section": "database",
          "import": "db/skeleton.sql"
        }
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Projet\\": "./Projet"
    }
  }
}
